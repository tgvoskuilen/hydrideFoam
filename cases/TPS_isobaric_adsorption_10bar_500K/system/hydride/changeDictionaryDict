FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      changeDictionaryDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dictionaryReplacement
{
    T
    {
        internalField   uniform 300;

        boundaryField
        {
            ".*"
            {
                type            zeroGradient;
                value           uniform 300;
            }

            "hydride_to_htx"
            {
                type            compressible::turbulentThermalContactResistance;
                neighbourFieldName T;
                K               lookup; //basicThermo;
                KName           keff; //none;
                value           uniform 300;
                Rtc             Rtc [-1 0 3 1 0 0 0] 0.005;
            }
        }
    }

    F
    {
        internalField   uniform 0;

        boundaryField
        {
            ".*"
            {
                type            zeroGradient;
            }

            "hydride_to_.*"
            {
                type            zeroGradient;
            }
        }
    }


    p
    {
        internalField   uniform 1e6;

        boundaryField
        {
            ".*"
            {
                type            zeroGradient;
            }

            Exterior
            {
                type            totalPressure;
                p0              uniform 1e6;
                U               U;
                phi             phi;
                rho             rho;
                psi             none;
                gamma           1;
                value           uniform 1e6;
            }
        }
    }
}

